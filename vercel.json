{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": null,
  "rewrites": [{ "source": "/", "destination": "/index.html" }],
  "headers": [{ "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }] }]
}
